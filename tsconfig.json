{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "esnext",
      "moduleResolution": "node",
      "target": "es5",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "lib": ["es2015", "dom"]
  },
  "include": [
      "./src/**/*"
  ]
}